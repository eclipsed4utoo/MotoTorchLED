<?xml version="1.0" encoding="utf-8"?> 
<LinearLayout android:id="@+id/main_pro"
	android:gravity="center"
    android:background="@drawable/widget_frame_portrait3x1" 
    xmlns:android="http://schemas.android.com/apk/res/android" 
    android:layout_height="74dip" 
    android:layout_width="220dip"> 
    
    
	<LinearLayout
		android:id="@+id/btn_led"
	    android:layout_width="52dip"
	    android:layout_height="52dip"
	    android:background="@drawable/appwidget_button_center"
	    android:clickable="true"
		android:focusable="true"
		android:gravity="center">
	
			<ImageView
				android:id="@+id/img_led"
				android:layout_height="40dip"
				android:layout_gravity="center"
				android:layout_width="40dip"
				android:src="@drawable/mototorch_led_off" 
				android:scaleType="fitXY"/>
				
	
	</LinearLayout>
 
	<LinearLayout
		android:id="@+id/btn_strobe"
	    android:layout_width="52dip"
	    android:layout_height="52dip"
	    android:background="@drawable/appwidget_button_center"
	    android:clickable="true"
		android:focusable="true"
		android:gravity="center">
	
			<ImageView
				android:id="@+id/img_strobe"
				android:layout_height="40dip"
				android:layout_gravity="center"
				android:layout_width="40dip"
				android:src="@drawable/mototorch_strobe_off"
				android:scaleType="fitXY"/>
				
	
	</LinearLayout>

	<LinearLayout
		android:id="@+id/btn_morse"
	    android:layout_width="52dip"
	    android:layout_height="52dip"
	    android:background="@drawable/appwidget_button_center"
	    android:clickable="true"
		android:focusable="true"
		android:gravity="center">
	
			<ImageView
				android:id="@+id/img_strobe"
				android:layout_height="40dip"
				android:layout_gravity="center"
				android:layout_width="40dip"
				android:src="@drawable/mototorch_strobe_off"
				android:scaleType="fitXY"/>
				
	
	</LinearLayout>
	
	<LinearLayout
		android:id="@+id/btn_app"
	    android:layout_width="52dip"
	    android:layout_height="52dip"
	    android:background="@drawable/appwidget_button_center"
	    android:clickable="true"
		android:focusable="true"
		android:gravity="center">
			
			<ImageView
				android:id="@+id/img_strobe"
				android:layout_height="40dip"
				android:layout_gravity="center"
				android:layout_width="40dip"
				android:src="@drawable/icon"
				android:scaleType="fitXY"/>
				
	
	</LinearLayout>	

</LinearLayout>